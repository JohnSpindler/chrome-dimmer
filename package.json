{
  "name": "chrome-dimmer",
  "version": "0.0.2",
  "description": "Dim any web page background and images in Chrome & Edge browsers using a simple UI",
  "author": "John Spindler",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/JohnSpindler/chrome-dimmer",
  "bugs": "https://github.com/JohnSpindler/chrome-dimmer/issues",
  "readme": "README.md",
  "changelog": "CHANGELOG.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnSpindler/chrome-dimmer.git"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "clean:dist": "[ -d dist ] && rm -rf dist/*",
    "reinstall": "[ -d node_modules ] && rm -rf node_modules/* && yarn install"
  },
  "devDependencies": {
    "@types/chrome": "0",
    "copy-webpack-plugin": "9",
    "prettier": "2",
    "ts-loader": "9",
    "typescript": "4",
    "web-ext": "^6.2.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "engines": {
    "node": "14",
    "yarn": "^1.22.x"
  },
  "prettier": {
    "filepath": ".prettierrc.json5"
  },
  "keywords": [
    "browser extension",
    "google chrome extension",
    "microsoft edge extension"
  ]
}
