{
  "name": "chrome-dimmer",
  "version": "0.0.1",
  "description": "Dim Google Chrome window backgrounds and images with ease",
  "author": "John Spindler",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/JohnSpindler/chrome-dimmer",
  "bugs": "https://github.com/JohnSpindler/chrome-dimmer/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnSpindler/chrome-dimmer.git"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "build:ts": "tsc --project tsconfig.build.json",
    "clean:dist": "[ -d dist ] && rm -rf dist/*",
    "reinstall": "[ -d node_modules ] && rm -rf node_modules/* && yarn install"
  },
  "devDependencies": {
    "@types/chrome": "^0.0",
    "@types/node": "^14.14.31",
    "copy-webpack-plugin": "^7.0.0",
    "path": "^0.12.7",
    "prettier": "^2.2",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "engines": {
    "node": "14",
    "yarn": "^1.22.x"
  },
  "prettier": {
    "filepath": ".prettierrc.json5"
  },
  "keywords": [
    "chrome extension",
    "chrome-extension"
  ]
}
